{
  "manifest_version": 3,
  "name": "Dont Read Later",
  "version": "1.0",
  "description": "Stop Bookmarking Start Reading",
  "author": "Kanasero",
  "action": {
    "default_title": "Dont Read Later",
    "default_popup": "index.html#/popup",
    "default_icon": "icons/icon16.png"
  },
  "permissions": [ "storage", "tabs" ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
